characters = {}
cops = {}

game_lost = false
game_won = false
welcome = true

empty = 0
platform = 1
ladder = 2
cop = 33
emma = 17
bg = 5

machine_frame = 0


function new_character(player_number, sprite, x, y, dx, dy)
	player = {}
	player.nr = nr --player_number, 0 for cops, 1 for player1, 2 for player2
	player.x = x
	player.y = y
	player.speed = 0.1
	player.dx = dx
	player.dy = dy
	player.spr = sprite
	player.width = 0.3 --half the actual width
	player.height = 0.4 --half the actual height
	player.frame = 0
	player.frames = 2
	
	add(characters, player)
	return player;
end




function _init()
	music(0)
	p1 = new_character(1, emma, 2.4, 14.5, 0, 0)
	--p2 = new_character(2, 3, 2.4, 6.5, 0, 0)
	cop1 = new_character(0, cop, 13.4, 14.5, -0.06, 0)
	cop2 = new_character(0, cop, 8.4, 11.5, 0.06, 0)
	cop3 = new_character(0, cop, 8.4, 8.5, -0.06, 0)
	cop4 = new_character(0, cop, 5.4, 8.5, -0.06, 0)
	cop5 = new_character(0, cop, 11.4, 5.5, 0.06, 0)
	cop6 = new_character(0, cop, 5.4, 5.5, -0.06, 0)
	cop7 = new_character(0, cop, 13.4, 2.5, -0.06, 0)
	cop8 = new_character(0, cop, 9.4, 2.5, 0.06, 0)
	cop9 = new_character(0, cop, 3.4, 2.5, -0.06, 0)
	add(cops, cop1)
	add(cops, cop2)
	add(cops, cop3)
	add(cops, cop4)
	add(cops, cop5)
	add(cops, cop6)
	add(cops, cop7)
	add(cops, cop8)
	add(cops, cop9)
	
end

function control_player(player, nr)
	local offset = 0
	if nr == 2 then
		offset = 8
	end

	player.dx = 0
	if (btn(0 + offset)) then 
		player.dx -= player.speed
	elseif(btn(1 + offset)) then 
		player.dx += player.speed
	else
		player.dx = 0
	end
	
	player.dy = 0
	if (btn(2 + offset)) then 
		player.dy -= player.speed
	elseif(btn(3 + offset)) then 
		player.dy += player.speed
	else
		player.dy = 0
	end
	
	--todo: check if player is falling
end

function solid(x, y)

 -- grab the cell value
 val=mget(x, y)
 
 -- check if flag 1 is set (the
 -- orange toggle button in the 
 -- sprite editor)
 return fget(val, 1)
 
end

function check_falling(x, y)
	val=mget(x, y+1)
	return fget(val, 3)
end

--check against nearby tiles
function solid_area(x,y,w,h)

 return 
  solid(x-w,y-h) or
  solid(x+w,y-h) or
  solid(x-w,y+h) or
  solid(x+w,y+h)
end

-- checks both walls and actors
function character_collide(player, dx, dy)
	if solid_area(player.x+dx,player.y+dy, player.width, player.height) then
		return true
	else
		return false
	end
	--return solid_actor(a, dx, dy) 
end

function move_player(player)
	-- x axis
	if not character_collide(player, player.dx, 0) then
		player.x += player.dx
	end
	
	-- y axis
	if not character_collide(player, 0, player.dy) then
		player.y += player.dy
	end
end

function move_cop(cop)
	if solid_area(cop.x + cop.dx, cop.y, cop.width, cop.height) then
			cop.dx *= -1
		end
		cop.x += cop.dx
end

function check_bust_p1(cop)
	if(abs(p1.x - cop.x) < p1.width + cop.width and 
	   abs(p1.y - cop.y) < p1.height + cop.height)then
		game_lost = true
	end
end

function check_win()
	if(abs(p1.x - 11) < p1.width + 1 and 
	   abs(p1.y - 2) < p1.height + 1) then
		game_won = true
	end
end

function _update()
	control_player(p1, 1)
	--control_player(p2, 2)
	--foreach(characters, move_character)
	if welcome then
		if btn(0) or btn(1) or btn(2) or btn(3) or btn(4) or btn(5) then
			welcome = false
		end
	end
	
	if not game_lost then
		if not game_won then 
			if not welcome then
				move_player(p1)
				foreach(cops, move_cop)
				foreach(cops, check_bust_p1)
				check_win()
				--check_bust_p1(cop1)
			end
		end
	end
	
	if(game_won) then
	
	end
	
	if game_lost then
		--game_lost screen
		if btn(0) or btn(1) or btn(2) or btn(3) or btn(4) or btn(5) then
			--reset()
		end
	end
end

function reset()
	game_lost = false
	welcome = true
	
	cops = {}
	character = {}
	p1 = nil
	cop1 = nil
	cop2 = nil
	cop3 = nil
	cls()
	
end


function draw_character(character)
	local vx = (character.x * 8) - 3
	local vy = (character.y * 8) - 4
	
	if character.dx == 0 and character.dy == 0 then
		character.frame = 0
		spr(character.spr, vx, vy)
	else
		character.frame += 0.3
		character.frame %= character.frames
		spr(1 + character.spr + character.frame, vx, vy, 1, 1, character.dx < 0)
	end
end


function animate_machine()
	machine_frame += 0.1
	machine_frame %= 2
	if machine_frame > 1 then
		mset( 11, 1, 7 )
		mset( 12, 1, 8 )
 	else
 		mset( 11, 1, 9 )
 		mset( 12, 1, 10 )
 	end
end

function _draw()
	cls()
	if not welcome then
		map(0, 0, 0, 0, 32, 32)
		foreach(characters, draw_character)
		if game_lost then
			spr(128, 48, 48, 4, 4)
		elseif game_won then
			spr(132, 48, 48, 4, 4)
		else
			animate_machine()
		end
		
		
	else
		map(16, 0, 0, 0, 32, 32)
	end
end

--e.g. map(0,0, 20,20, 4,2)
--	-> draws a 4x2 blocks of cels starting from 0,0 in the map, to the screen at 20,20

