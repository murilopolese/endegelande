pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- ende gelÃ¤nde
-- by murilo and jonathan

empty = 0
platform = 1
ladder = 2
cop = 4
bg = 5

player1 = {}
player1.sprite = 3
player1.x = 3*8
player1.y = 3*8
player1.speed = 1

function get_cel( player )
	cell = {};
	--cell.x = flr( ( player.x + 4 ) / 8 )
	--cell.y = flr( ( player.y + 4 ) / 8 )
	cell.x = flr( ( player.x + 3) / 8 )
	cell.y = flr( ( player.y ) / 8 )
	return cell
end

function player_move( player )
	cel = get_cel( player1 )
	
	-- if you are on top of a platform, you can walk sideways
	if( mget( cel.x, cel.y + 1 ) != bg ) then
		if( mget( cel.x, cel.y ) == ladder) then
			if(btn( 2,player ) ) then
				player1.y -= player1.speed
			end
		end

		if( btn( 0,player) ) then
			player1.x -= player1.speed
		end
		if(btn(1,player)) then
			player1.x += player1.speed
		end
		
	else	
    --player falling down
	end
	
	
	
end

--i: 0..5: left right up down button_o button_x
function _update()
	player_move( 0 );
	
	cel = get_cel( player1 );
	mset( 10, 12, mget( cel.x, cel.y - 1 ) )
	mset( 10, 13, mget( cel.x, cel.y ) )
	mset( 10, 14, mget( cel.x, cel.y + 1 ) )
	mset( 9, 13, mget( cel.x - 1, cel.y ) )
	mset( 11, 13, mget( cel.x + 1, cel.y ) )
end

function _draw()
	map(0, 0, 0, 0, 25, 25)
	spr( player1.sprite, player1.x, player1.y )
end

--e.g. map(0,0, 20,20, 4,2)
--	-> draws a 4x2 blocks of cels starting from 0,0 in the map, to the screen at 20,20

